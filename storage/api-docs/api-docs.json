{
    "openapi": "3.0.0",
    "info": {
        "title": "Swagger-demo",
        "description": "Swagger `Api of condominiums`",
        "contact": {
            "email": "swagger@gmail.com"
        },
        "version": "1.0.0"
    },
    "servers": [
        {
            "url": "http://localhost:8000/condominiums"
        }
    ],
    "paths": {
        "/login": {
            "post": {
                "tags": [
                    "Users"
                ],
                "summary": "Login",
                "description": "Login Users",
                "operationId": "App\\Http\\Controllers\\LoginController::login",
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/schemas/Users"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "The Post resource created",
                        "content": {
                            "application/json": {}
                        }
                    },
                    "401": {
                        "description": "Unauthenticated.",
                        "content": {
                            "application/json": {}
                        }
                    },
                    "default": {
                        "description": "an \"unexpected\" error",
                        "content": {
                            "application/json": {}
                        }
                    }
                },
                "security": [
                    {
                        "passport": [
                            "*"
                        ]
                    }
                ]
            }
        },
        "/users": {
            "get": {
                "tags": [
                    "Users"
                ],
                "summary": "Lists available Users",
                "description": "Gets all available Users resources",
                "operationId": "App\\Http\\Controllers\\UsersController::index",
                "parameters": [
                    {
                        "name": "paginate",
                        "in": "query",
                        "description": "paginate",
                        "required": false,
                        "schema": {
                            "title": "Paginate",
                            "description": "The unique identifier of a Post resource",
                            "type": "boolean",
                            "example": "true"
                        }
                    },
                    {
                        "name": "dataSearch",
                        "in": "query",
                        "description": "Post resource name",
                        "required": false,
                        "schema": {
                            "description": "The unique identifier of a Post resource",
                            "type": "string"
                        }
                    },
                    {
                        "name": "sortField",
                        "in": "query",
                        "description": "Sort field",
                        "required": false,
                        "schema": {
                            "title": "name",
                            "description": "The unique identifier of a Post resource",
                            "type": "string",
                            "example": "name"
                        }
                    },
                    {
                        "name": "sortOrder",
                        "in": "query",
                        "description": "Sort order field",
                        "schema": {
                            "title": "sortOrder",
                            "description": "The unique identifier of a Post resource",
                            "type": "string",
                            "example": "asc"
                        }
                    },
                    {
                        "name": "perPage",
                        "in": "query",
                        "description": "Sort order field",
                        "schema": {
                            "title": "perPage",
                            "description": "The unique identifier of a Post resource",
                            "type": "number",
                            "default": "0"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "A list with Users",
                        "content": {
                            "application/json": {}
                        }
                    },
                    "401": {
                        "description": "Unauthenticated.",
                        "content": {
                            "application/json": {}
                        }
                    },
                    "default": {
                        "description": "an \"unexpected\" error",
                        "content": {
                            "application/json": {}
                        }
                    }
                },
                "security": [
                    {
                        "passport": [
                            "*"
                        ]
                    }
                ]
            }
        }
    },
    "components": {
        "schemas": {
            "Users": {
                "properties": {
                    "email": {
                        "description": "The Post's email",
                        "required": [
                            "true"
                        ],
                        "type": "string"
                    },
                    "password": {
                        "description": "The email password",
                        "required": [
                            "true"
                        ],
                        "type": "string"
                    }
                },
                "type": "object"
            }
        }
    }
}