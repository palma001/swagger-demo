{
    "openapi": "3.0.0",
    "info": {
        "title": "Swagger-demo",
        "description": "Swagger `Api of condominiums`",
        "contact": {
            "email": "swagger@gmail.com"
        },
        "version": "1.0.0"
    },
    "servers": [
        {
            "url": "http://localhost:8000/condominiums"
        }
    ],
    "paths": {
        "/authenticate": {
            "post": {
                "tags": [
                    "Users"
                ],
                "summary": "Authenticate",
                "description": "Authenticate Users",
                "operationId": "App\\Http\\Controllers\\AuthenticateController::authenticate",
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/schemas/Login"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "The Post resource created",
                        "content": {
                            "application/json": {}
                        }
                    },
                    "401": {
                        "description": "Unauthenticated.",
                        "content": {
                            "application/json": {}
                        }
                    },
                    "default": {
                        "description": "an \"unexpected\" error",
                        "content": {
                            "application/json": {}
                        }
                    }
                },
                "security": [
                    {
                        "passport": [
                            "*"
                        ]
                    }
                ]
            }
        },
        "/users": {
            "get": {
                "tags": [
                    "Users"
                ],
                "summary": "Lists available Users",
                "description": "Gets all available Users resources",
                "operationId": "App\\Http\\Controllers\\UsersController::index",
                "parameters": [
                    {
                        "name": "paginate",
                        "in": "query",
                        "description": "paginate",
                        "required": false,
                        "schema": {
                            "title": "Paginate",
                            "description": "The unique identifier of a Post resource",
                            "type": "boolean",
                            "example": "true"
                        }
                    },
                    {
                        "name": "dataSearch",
                        "in": "query",
                        "description": "Post resource name",
                        "required": false,
                        "schema": {
                            "description": "The unique identifier of a Post resource",
                            "type": "string"
                        }
                    },
                    {
                        "name": "sortField",
                        "in": "query",
                        "description": "Sort field",
                        "required": false,
                        "schema": {
                            "title": "name",
                            "description": "The unique identifier of a Post resource",
                            "type": "string",
                            "example": "name"
                        }
                    },
                    {
                        "name": "sortOrder",
                        "in": "query",
                        "description": "Sort order field",
                        "schema": {
                            "title": "sortOrder",
                            "description": "The unique identifier of a Post resource",
                            "type": "string",
                            "example": "asc"
                        }
                    },
                    {
                        "name": "perPage",
                        "in": "query",
                        "description": "Sort order field",
                        "schema": {
                            "title": "perPage",
                            "description": "The unique identifier of a Users resource",
                            "type": "number",
                            "default": "0"
                        }
                    },
                    {
                        "name": "authorization",
                        "in": "header",
                        "description": "authorization",
                        "schema": {
                            "title": "authorization",
                            "type": "string"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "A list with Users",
                        "headers": {
                            "X-Auth-Token": {
                                "description": "calls per hour allowed by the user",
                                "schema": {
                                    "type": "integer",
                                    "format": "int32"
                                }
                            }
                        },
                        "content": {
                            "application/json": {}
                        }
                    },
                    "401": {
                        "description": "Unauthenticated.",
                        "content": {
                            "application/json": {}
                        }
                    },
                    "default": {
                        "description": "an \"unexpected\" error",
                        "content": {
                            "application/json": {}
                        }
                    }
                },
                "security": [
                    {
                        "passport": [
                            "*"
                        ]
                    }
                ]
            }
        }
    },
    "components": {
        "schemas": {
            "Login": {
                "properties": {
                    "email": {
                        "description": "The Users email",
                        "required": [
                            "true"
                        ],
                        "type": "string"
                    },
                    "password": {
                        "description": "The email password",
                        "required": [
                            "true"
                        ],
                        "type": "string"
                    }
                },
                "type": "object"
            },
            "User": {
                "properties": {
                    "lastname": {
                        "description": "The user lastname",
                        "required": [
                            "true"
                        ],
                        "type": "string"
                    },
                    "ci": {
                        "description": "The user ci",
                        "required": [
                            "true"
                        ],
                        "type": "string"
                    },
                    "email": {
                        "description": "The Users email",
                        "required": [
                            "true"
                        ],
                        "type": "string"
                    },
                    "tlf": {
                        "description": "The Users tlf",
                        "required": [
                            "true"
                        ],
                        "type": "string"
                    },
                    "state": {
                        "description": "The Users state",
                        "required": [
                            "true"
                        ],
                        "type": "string"
                    },
                    "password": {
                        "description": "The Users password",
                        "required": [
                            "true"
                        ],
                        "type": "string"
                    }
                },
                "type": "object"
            }
        }
    }
}